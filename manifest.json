{
  "name": "PageLater",
  "description": "Productivity Tabs Scheduler",
  "version": "1.0",
  "manifest_version": 2,

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png",
    "default_title": "Open a URL"
  },

  "permissions": ["tabs"],

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
